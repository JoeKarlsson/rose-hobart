{
  "name": "rose-hobart",
  "version": "1.0.0",
  "description": "A digital humanities project to explore the (lack of) performativity in Joseph Cornell's short film Rose Hobart",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "node server.js",
    "start": "npm run build && npm run serve",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3",
    "gl-react": "^5.2.0",
    "gl-react-dom": "^5.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.0.4",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "jsdom": "^27.0.0",
    "sass": "^1.77.8"
  },
  "keywords": [
    "react",
    "digital-humanities",
    "rose-hobart",
    "video",
    "interactive"
  ],
  "author": "Joe Karlson and Laurel Karlsson",
  "license": "ISC"
}
