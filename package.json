{
  "name": "rose-hobart",
  "version": "1.0.0",
  "description": "A digital humanities project to explore the (lack of) performativity in Joseph Cornell's short film Rose Hobart",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "node server.js",
    "start": "npm run build && npm run serve",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:preview": "npm run build && gh-pages -d dist --branch preview",
    "deploy:setup": "gh-pages -d dist --add",
    "deploy:vite": "vite build && gh-pages -d dist"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^5.0.4",
    "gh-pages": "^6.1.1",
    "jsdom": "^27.0.0",
    "sass": "^1.77.8",
    "vite": "^7.1.7",
    "vite-plugin-gh-pages": "^1.0.1",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "react",
    "digital-humanities",
    "rose-hobart",
    "video",
    "interactive"
  ],
  "author": "Joe Karlson and Laurel Karlsson",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoeKarlsson/rose-hobart.git"
  },
  "homepage": "https://joeKarlsson.github.io/rose-hobart",
  "bugs": {
    "url": "https://github.com/JoeKarlsson/rose-hobart/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
